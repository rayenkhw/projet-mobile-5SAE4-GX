<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homebkg1"
    tools:context=".EvaluationDetail">

    <!-- Add UI elements to display enseignant details -->

    <EditText
        android:id="@+id/noteExamenTextView"
        android:layout_width="128dp"
        android:layout_height="64dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="32dp"
        android:inputType="text"
        android:text="note examen"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/noteCcTextView"
        android:layout_width="145dp"
        android:layout_height="57dp"
        android:layout_below="@id/noteExamenTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:inputType="text"
        android:text="note cc"
        android:textSize="18sp" />

    <EditText
        android:id="@+id/remarqueTextView"
        android:layout_width="146dp"
        android:layout_height="68dp"
        android:layout_below="@id/noteCcTextView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:inputType="textEmailAddress"
        android:text="email"
        android:textSize="18sp" />




    <Button
        android:id="@+id/buttonModifier1"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginTop="75dp"
        android:layout_marginEnd="37dp"
        android:text="Modifier" />

    <Button
        android:id="@+id/buttonDelete1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/buttonModifier1"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="71dp"
        android:layout_marginTop="-50dp"
        android:text="Supprimer" />

</RelativeLayout>
